{
  "extends": "standard",
  "rules": {
    "camelcase": [ 2, { "properties": "always" } ],
    "complexity": [ 2, 6 ],
    "curly": [ 2, "multi-line" ],
    "indent": 0,
    "semi": [2, "never"],
    "max-len": [ 2, 120, 2 ],
    "max-nested-callbacks": [ 2, 10 ],
    "max-params": [ 2, 7 ],
    "max-statements": [ 2, 25 ],
    "no-unused-vars": [ 2, "all" ],
    "one-var": [ 2, "always" ],
    "padded-blocks": 0
  }
}
